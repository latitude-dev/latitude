{
  "name": "@latitude-sdk/svelte",
  "version": "0.0.1",
  "scripts": {
    "dev:dummyApp": "vite dev",
    "test": "vitest",
    "lint": "eslint . --max-warnings 0",
    "tc": "tsc --noEmit",
    "prettier": "prettier --write src/**/*.ts src/**/*.svelte",
    "storybook": "storybook dev -p 6006",
    "prepare": "svelte-kit sync",
    "package": "pnpm run prepare && svelte-package && publint",
    "build-storybook": "storybook build",
    "build:vite": "vite build",
    "prebuild": "node ./scripts/hack-svelte-package/prebuild.js",
    "buildAndPackage": "pnpm run build:vite && pnpm run package",
    "build:with-prebuild-not-used-yet": "pnpm run prebuild && pnpm run buildAndPackage",
    "build": "pnpm run buildAndPackage",
    "prepublishOnly": "pnpm run package"
  },
  "type": "module",
  "exports": {
    "./tailwind.preset": "./tailwind.config.js",
    ".": {
      "types": "./src/lib/index.ts",
      "import": "./src/lib/index.ts"
    },
    "./*": {
      "types": "./src/lib/ui/*/index.ts",
      "import": "./src/lib/ui/*/index.ts",
      "svelte": "./src/lib/ui/*/index.svelte"
    }
  },
  "files": [
    "src",
    "tailwind.config.js",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.stories.svelte"
  ],
  "dependencies": {
    "@latitude-sdk/client": "workspace:*",
    "bits-ui": "^0.18.1",
    "mode-watcher": "^0.2.1",
    "radix-icons-svelte": "^1.2.1",
    "svelte-persisted-store": "^0.9.1"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@latitude-sdk/eslint-config": "workspace:*",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-svelte-csf": "^4.1.1",
    "@storybook/blocks": "^7.6.16",
    "@storybook/svelte": "^7.6.16",
    "@storybook/sveltekit": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-local-rules": "^2.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "publint": "^0.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.16",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}
