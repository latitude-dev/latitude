{
  "name": "@latitude-data/svelte",
  "version": "0.7.0-next.3",
  "license": "LGPL",
  "scripts": {
    "build": "pnpm run buildAndPackage",
    "build-storybook": "storybook build",
    "build:vite": "vite build",
    "build:css": "npx tailwindcss -i ./src/assets/app.css -o ./dist/styles.css",
    "buildAndPackage": "pnpm run build:vite && pnpm run package && pnpm run build:css",
    "dev": "svelte-package --watch",
    "dev:vite": "vite dev",
    "lint": "eslint . --max-warnings 0",
    "package": "pnpm run prepare && svelte-package && publint",
    "prepare": "svelte-kit sync",
    "prettier": "prettier --write src/**/*.ts src/**/*.svelte",
    "storybook": "storybook dev -p 6006",
    "tc": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest --run"
  },
  "type": "module",
  "exports": {
    "./tailwind.preset": "./tailwind.config.js",
    "./internal": {
      "types": "./dist/internal/index.d.ts",
      "svelte": "./dist/internal/index.js",
      "default": "./dist/internal/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "typesVersions": {
    ">4.0": {
      "*": [
        "./dist/index.d.ts"
      ],
      "internal": [
        "./dist/internal/index.d.ts"
      ]
    }
  },
  "files": [
    "tailwind.config.js",
    "dist",
    "dist/internal",
    "!dist/**/*.test.*",
    "!dist/**/*.stories.svelte"
  ],
  "dependencies": {
    "@internationalized/date": "^3.5.2",
    "@latitude-data/client": "workspace:*",
    "@latitude-data/custom_types": "workspace:^",
    "@latitude-data/query_result": "workspace:^",
    "@types/lodash-es": "^4.17.12",
    "bits-ui": "^0.21.1",
    "cmdk-sv": "^0.0.16",
    "echarts": "^5.5.0",
    "lodash-es": "^4.17.21",
    "mode-watcher": "^0.2.1",
    "radix-icons-svelte": "^1.2.1",
    "svelte-headless-table": "^0.18.2",
    "svelte-persisted-store": "^0.9.1",
    "zustand": "^4.5.1"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@latitude-data/eslint-config": "workspace:*",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-svelte-csf": "^4.1.1",
    "@storybook/blocks": "^7.6.17",
    "@storybook/svelte": "^7.6.17",
    "@storybook/sveltekit": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/echarts": "^4.9.22",
    "@types/eslint": "8.56.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "publint": "^0.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.6.17",
    "storybook-addon-deep-controls": "^0.6.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "zustand": "^4.5.1"
  }
}
