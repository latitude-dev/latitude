  "license": "LGPL",
  "scripts": {
    "build:rollup": "rollup -c",
    "build:css": "npx tailwindcss -i ./src/assets/app.css -o ./dist/styles.css",
    "build": "npm run build:rollup && npm run build:css",
    "dev": "pnpm run build:rollup -w",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint . --max-warnings 0 --ext .ts src",
    "tc": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "dependencies": {
    "@latitude-data/client": "workspace:*",
    "@latitude-data/embedding": "workspace:*",
    "@latitude-data/webcomponents": "workspace:*",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.28.14",
    "@tanstack/react-table": "^8.16.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "echarts": "^5.5.0",
    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "npm": "^10.5.2",
    "react-table": "^7.8.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@latitude-data/eslint-config": "workspace:*",
    "@latitude-data/query_result": "workspace:^",
    "@latitude-data/typescript": "workspace:*",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^14.2.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/eslint": "8.56.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@types/react-table": "^7.7.20",
    "cross-fetch": "^4.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.13",
    "prettier": "^3.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.10.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.0",
    "vitest": "^1.4.0"
  }
